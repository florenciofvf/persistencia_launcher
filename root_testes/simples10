defun se($valor, $retorno1, $retorno2) {
	if($valor > 2222) {
		return ($retorno1);
	} else {
		return ($retorno2);
	}
}

defun teste($valor, $retorno1, $retorno2) {
	if($valor > 2222) {
		mensagem($retorno1);
	} else {
		mensagem($retorno2);
	}
	return (0);
}

defun mensagem(valor) : void {
	return;
